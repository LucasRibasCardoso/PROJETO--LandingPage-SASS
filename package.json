{
  "name": "sass",
  "version": "1.0.0",
  "description": "modulo de sass",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass sass/main.scss css/style.css -w",
    "compile:sass": "sass sass/main.scss css/style.comp.css",
    "prefix:sass": "postcss --use autoprefixer -b 'last 10 versions' css/style.comp.css css/style.prefix.csss",
    "compress:sass": "sass css/style.prefix.css css/style.css --style compressed",
    "build:sass": "npm run-all compile:sass prefix:css compress:css"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "sass": "^1.77.2"
  }
}